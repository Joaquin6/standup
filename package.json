{
  "name": "GitCreator",
  "version": "0.1.0",
  "description": "Help the development to easily create Git repos, along with Jenkins and Jira project spaces.",
  "main": "server/server.js",
  "author": "Mirium Firestarter Team",
  "license": "ISC",
  "engines": {
    "node": ">=6.2.1",
    "npm": "~2.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Joaquin6/standup"
  },
  "scripts": {
    "env": "env",
    "inspector": "node-inspector",
    "autoprefixer": "postcss -u --local-plugins -u autoprefixer -m file -r ./app/css/firestarter.css",
    "clean:sasscache": "rimraf .sass-cache -s",
    "clean:npmlog": "rimraf npm-debug.log -s",
    "clean:lintlog": "rimraf eslint-debug.log -s",
    "clean:release": "rimraf release -s",
    "clean:nodemods": "npm run clean:static && rimraf node_modules -s",
    "clean:static": "rimraf static -s",
    "clean:logs": "npm run clean:lintlog && npm run clean:npmlog",
    "preclean": "npm run clean:sasscache",
    "clean": "npm run clean:logs && npm run clean:release",
    "postclean": "npm run clean:nodemods",
    "build": "webpack -p --progress --colors",
    "postbuild": "npm run copy:images",
    "sass": "node-sass --output-style nested --indent-type tab --indent-width 1 ./app/sass/firestarter.scss ./app/css/firestarter.css",
    "postsass": "npm run autoprefixer",
    "sass:watch": "npm run sass -- -w ./app/sass",
    "server": "node ./server/server.js",
    "server-debug": "node --debug ./server/server.js &",
    "prestart": "npm run build",
    "start": "npm run server",
    "stop": "killall node",
    "start-dev": "NODE_ENV= npm run server & npm run sass:watch",
    "start-qa": "NODE_ENV=qa npm start",
    "start-prod": "NODE_ENV=prod npm start",
    "start-dev-debug": "NODE_ENV= npm run server-debug & npm run sass:watch",
    "start-qa-debug": "NODE_ENV=qa npm run server-debug",
    "start-prod-debug": "NODE_ENV=prod npm run server-debug",
    "prerestart": "npm stop",
    "restart": "npm run start-dev",
    "prerestart-debug": "npm stop",
    "restart-debug": "npm run start-dev-debug",
    "copy:images": "sh ./app/build/copyImages.sh",
    "lint": "eslint -f table app/js/ > eslint-debug.log",
    "test": "npm run lint",
    "unit-test": "./node_modules/mocha/bin/_mocha --recursive spec/Tests",
    "code-coverage": "./node_modules/.bin/istanbul cover -- ./node_modules/mocha/bin/_mocha --recursive spec/Tests"
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "amd": true,
      "commonjs": true,
      "mocha": true,
      "node": true,
      "jquery": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "rules": {
      "no-func-assign": "error",
      "use-isnan": "error",
      "no-unexpected-multiline": "error"
    }
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "clarify": "^2.0.0",
    "cli-color": "^1.1.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "es5-shim": "^4.5.8",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-release": "^0.5.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "highcharts": "^4.2.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "http-proxy": "^1.13.2",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
    "js-cookie": "^2.1.2",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.11.2",
    "massive": "^2.5.0",
    "method-override": "^2.3.5",
    "moment": "^2.13.0",
    "moment-range": "^2.2.0",
    "multer": "^1.1.0",
    "nanoscroller": "^0.8.7",
    "node-sass": "^3.7.0",
    "node-uuid": "^1.4.7",
    "npm": "^3.8.9",
    "npm-license": "^0.3.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sockjs": "^0.3.17",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.0",
    "webpack-error-notification": "^0.1.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "postcss-cli": "^2.5.2",
    "rewire": "^2.5.1",
    "stack-trace": "0.0.9",
    "supertest": "^1.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "1.10.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
