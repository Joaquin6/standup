{
  "name": "standup",
  "version": "0.1.0",
  "description": "Assist developers in creating git repos and integrate with jenkins and jira",
  "main": "server/server.js",
  "author": "Joaquin Briceno",
  "license": "ISC",
  "engines": {
    "node": ">=6.2.1",
    "npm": "~2.15.1"
  },
  "scripts": {
    "env": "env",
    "inspector": "node-inspector",
    "autoprefixer": "postcss -u --local-plugins -u autoprefixer -m file -r ./app/css/visionwheel.css",
    "build": "webpack -p --progress --colors",
    "postbuild": "npm run copy:images",
    "sass": "node-sass --output-style nested --indent-type tab --indent-width 1 ./app/sass/visionwheel.scss ./app/css/visionwheel.css",
    "postsass": "npm run autoprefixer",
    "sass:watch": "npm run sass -- -w ./app/sass",
    "server": "node ./server/server.js",
    "server-debug": "node --debug ./server/server.js &",
    "server-debug-brk": "node --debug-brk ./server/server.js &",
    "data-import": "node ./server/libs/importer/index.js &",
    "data-import-debug": "node --debug ./server/libs/importer/index.js &",
    "data-import-debug-brk": "node --debug-brk ./server/libs/importer/index.js &",
    "prestart": "npm run build",
    "start": "npm run server",
    "start-dev": "NODE_ENV= npm run server & npm run sass:watch",
    "start-dev-debug": "NODE_ENV= Q_DEBUG=1 npm run server-debug & npm run sass:watch",
    "start-dev-debug-brk": "NODE_ENV= Q_DEBUG=1 npm run server-debug-brk & npm run sass:watch",
    "restart": "npm run start-dev",
    "restart-debug": "npm run start-dev-debug",
    "restart-debug-brk": "npm run server-debug-brk",
    "importer": "npm run data-import",
    "importer-debug": "npm run data-import-debug",
    "importer-debug-brk": "npm run data-import-debug-brk",
    "copy:images": "sh ./app/build/copyImages.sh",
    "lint": "eslint -f table app/js/ > eslint-debug.log",
    "test": "npm run lint",
    "unit-test": "./node_modules/mocha/bin/_mocha --recursive spec/Tests",
    "code-coverage": "./node_modules/.bin/istanbul cover -- ./node_modules/mocha/bin/_mocha --recursive spec/Tests"
  },
  "repository": {
    "type": "git",
    "url": "git@int-gitlab.aws.lbox.com:Mirum/vision-wheel-dealers.git"
  },
  "keywords": [
    "vision",
    "wheel",
    "dealers"
  ],
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "amd": true,
      "commonjs": true,
      "mocha": true,
      "node": true,
      "jquery": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "rules": {
      "no-func-assign": "error",
      "use-isnan": "error",
      "no-unexpected-multiline": "error"
    }
  },
  "dependencies": {
    "async": "^1.5.2",
    "aws-sdk": "^2.4.9",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "backbone": "^1.3.3",
    "backbone.marionette": "^3.0.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.0",
    "camelcase": "^3.0.0",
    "clarify": "^2.0.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "custom-err": "0.0.2",
    "es5-shim": "^4.5.8",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-release": "^0.5.0",
    "express": "^4.13.4",
    "express-session": "^1.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "http-proxy": "^1.13.2",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.3",
    "js-cookie": "^2.1.2",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.11.2",
    "massive": "^2.4.0",
    "method-override": "^2.3.5",
    "moment": "^2.13.0",
    "moment-range": "^2.2.0",
    "node-sass": "^3.7.0",
    "node-uuid": "^1.4.7",
    "npm": "^3.8.9",
    "npm-license": "^0.3.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "q": "^1.4.1",
    "raw-loader": "^0.5.1",
    "requirejs": "^2.2.0",
    "requirejs-text": "^2.0.12",
    "sass-loader": "^3.2.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "util": "^0.10.3",
    "webpack": "^1.13.0",
    "webpack-error-notification": "^0.1.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "node-debug": "^0.1.0",
    "node-inspector": "^0.12.8",
    "postcss-cli": "^2.5.2",
    "rewire": "^2.5.1",
    "stack-trace": "0.0.9",
    "supertest": "^1.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "1.10.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
